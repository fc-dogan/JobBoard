@using JobBoard.Models;

<h1>Job Board</h1>
<ul>
@if (Model.Count == 0)
{
  <p>There are no jobs in the list.</p>
}
</ul>
<ul>
  @foreach (JobOpening item in Model)
  {
    <li>@item.Title</li>
    <p>@item.Description</p>
  
    }
  
  @foreach (Contact citem in Model)
    {
      <li>@citem.Name</li>
      <li>@citem.Email</li>
      <li>@citem.Phone</li>
    }
</ul>

<a href="/jobopenings/new">Add a new item.</a>